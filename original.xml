<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC "-//Uppaal Team//DTD Flat System 1.1//EN" "lib/flat-1_1.dtd">
<nta>
  <declaration>
int r1 = 0;
int r2 = 0;
void ISend(int p0, int p1) {
}
void IReceive(int p0, int p1) {
}
void ITau() {
}
void IDummy(int p0, int p1) {
}
void OOK() {
}
void OFINAL() {
}
</declaration>
    <template>
    <name x="5" y="5">Template</name>
    
    <declaration></declaration>
    
    <location id="s0" x="0" y="0">
      <name >s0</name>
      
      
    </location>

    <location id="s1" x="0" y="0">
      <name >s1</name>
      
      
    </location>

    <location id="q0" x="0" y="0">
      <name >q0</name>
      
      
    </location>

    <location id="p1" x="0" y="0">
      <name >p1</name>
      
      
    </location>

    <location id="q1" x="0" y="0">
      <name >q1</name>
      
      
    </location>

    <location id="p2" x="0" y="0">
      <name >p2</name>
      
      
    </location>

    <location id="q2" x="0" y="0">
      <name >q2</name>
      
      
    </location>

    <location id="q3" x="0" y="0">
      <name >q3</name>
      
      
    </location>

    <location id="p3" x="0" y="0">
      <name >p3</name>
      
      
    </location>
    
    <init ref="s0" />
    
    <transition>
      <source ref="s0" />
      <target ref="s1" />
      
      <label kind="guard"> x1!=x2
      </label>
      <label kind="synchronisation">IDummy(x1,x2)</label>
      <label kind="assignment">r1=x1;r2=x2;</label>
      
    </transition>

    <transition>
      <source ref="s1" />
      <target ref="q0" />
      
      
      <label kind="synchronisation">OOK()</label>
      
      
    </transition>

    <transition>
      <source ref="q0" />
      <target ref="p1" />
      
      <label kind="guard">r1==x1 &amp;&amp; (r2!=x1 &amp;&amp; r2!=x2)</label>
      <label kind="synchronisation">ISend(x1,x2)</label>
      <label kind="assignment">r2=x2;</label>
      
    </transition>

    <transition>
      <source ref="q1" />
      <target ref="p3" />
      
      <label kind="guard"> r1==x1
      </label>
      <label kind="synchronisation">IReceive(x1,x2)</label>
      
      
    </transition>

    <transition>
      <source ref="q0" />
      <target ref="p2" />
      
      <label kind="guard"> r1==x1
      </label>
      <label kind="synchronisation">IReceive(x1,x2)</label>
      <label kind="assignment">r2=x2;</label>
      
    </transition>

    <transition>
      <source ref="q2" />
      <target ref="p3" />
      
      <label kind="guard"> r1==x1 &amp;&amp; (r1!=x2 &amp;&amp; r2!=x2)
      </label>
      <label kind="synchronisation">ISend(x1,x2)</label>
      
      
    </transition>

    <transition>
      <source ref="q0" />
      <target ref="p3" />
      
      
      <label kind="synchronisation">ITau()</label>
      
      
    </transition>

    <transition>
      <source ref="p1" />
      <target ref="q1" />
      
      
      <label kind="synchronisation">OOK()</label>
      
      
    </transition>

    <transition>
      <source ref="p2" />
      <target ref="q2" />
      
      
      <label kind="synchronisation">OOK()</label>
      
      
    </transition>

    <transition>
      <source ref="p3" />
      <target ref="q3" />
      
      
      <label kind="synchronisation">OFINAL()</label>
      
      
    </transition>

    <transition>
      <source ref="q3" />
      <target ref="p3" />
      
      
      <label kind="synchronisation">IDummy(x1,x2)</label>
      
      
    </transition>
  </template>

  <system>Process = Template();system Process;</system>
</nta>